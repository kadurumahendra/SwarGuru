<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swarguru"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="34" />

    <permission
        android:name="com.example.swarguru.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.swarguru.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.SwarGuru" >
        <activity
            android:name="com.example.swarguru.AdminActivity.AdminDashboardActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.QuizActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.SpeechTherapyActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.ChangePasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.EditProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.AnimationActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.LetterWordsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.LettersActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.LevelActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.DashboardActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.TherapistActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.WordsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.AgeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.SpeechLevelActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.LanguageActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.SignupActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.example.swarguru.WelcomeActivity"
            android:exported="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.swarguru.MainActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.swarguru.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.profileinstaller.ProfileInstallerInitializer"
                android:value="androidx.startup" />
        </provider>

        <receiver
            android:name="androidx.profileinstaller.ProfileInstallReceiver"
            android:directBootAware="false"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.DUMP" >
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.INSTALL_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SKIP_FILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SAVE_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.BENCHMARK_OPERATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>